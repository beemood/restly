{
  "name": "@restly/cli",
  "version": "0.0.1",
  "displayName": "Cli tool",
  "description": "Cli tool that provides generators",
  "keywords": [
    "cli",
    "plugin",
    "generator",
    "command"
  ],
  "homepage": "https://beemood.github.io/restly/libs/cli",
  "icon": "https://beemood.github.io/restly/libs/cli/assets/favicon.png",
  "author": {
    "name": "Robert Brightline",
    "email": "robert-brightline@gmail.com",
    "url": "https://beemood.github.io/profile"
  },
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bugs": {
    "email": "robert-brightline@gmail.com"
  },
  "sponsor": {
    "url": "beemood.github.io"
  },
  "bin": {
    "restly": "./bin/restly.cjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@restly/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json"
  ],
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/cli/dist",
          "main": "libs/cli/src/index.ts",
          "tsConfig": "libs/cli/tsconfig.lib.json",
          "rootDir": "libs/cli/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./libs/cli/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./libs/cli/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "21.5.1",
    "commander": "^14.0.0",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json",
  "devDependencies": {
    "@nx/nx-plugin": "16.0.0-beta.1"
  }
}

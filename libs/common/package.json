{
  "version": "0.0.1",
  "name": "@restly/common",
  "displayName": "common",
  "description": "common",
  "license": "MIT",
  "keywords": [
    "beenest"
  ],
  "homepage": "https://beemood.github.io/restly/libs/common",
  "icon": "https://beemood.github.io/restly/libs/common/assets/favicon.png",
  "author": {
    "name": "Robert Brightline",
    "email": "robert-brightline@gmail.com",
    "url": "https://beemood.github.io/profile"
  },
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bugs": {
    "email": "robert-brightline@gmail.com"
  },
  "sponsor": {
    "url": "https://beemood.github.io"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/common/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/common/dist",
          "main": "libs/common/src/index.ts",
          "tsConfig": "libs/common/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        },
        "dependsOn": [
          "build-deps"
        ]
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "peerDependencies": {
    "@apollo/server": "^4.12.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.1.6",
    "@nestjs/throttler": "^6.4.0",
    "cache-manager": "^7.2.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.5"
  },
  "dependencies": {
    "@restly/names": "workspace:*",
    "@swc/helpers": "~0.5.11"
  },
  "devDependencies": {
    "@restly/types": "workspace:*",
    "@types/express": "^5.0.3"
  }
}
